<h2>New projects</h2>

<%= form_for :project, url: projects_path do |f| %>
    <p>
        <%= f.label :name %><br>
        <%= f.text_field :name %>
    </p>
    
    <p>
        <%= f.label :summary %><br>
        <%= f.text_area :summary %>
    </p>
    
    <p>
        <%= f.label :start_date %><br>
        <%= f.date_field :start_date %>
    </p>
    
    <p>
        <%= f.label :end_date %><br>
        <%= f.date_field :end_date %>
    </p>
    
    <p>
        <%= f.submit %>
    </p>
<% end %>