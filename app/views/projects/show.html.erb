<h2><%= @project.name %></h2>
<p><%= @project.summary %></p>
<p><%= @project.start_date %> - <%= @project.end_date %></p>
<hr>
<%= link_to "Edit", edit_project_path(@project), :class => 'btn btn-outline-secondary'%>
<%= link_to "Add task", new_project_task_path(project_id: @project.id), :class => "btn btn-outline-dark"%>
<%= link_to "Delete", project_path(@project), method: :delete, data: {confirm: "Delete project?"}, :class => 'btn btn-outline-danger'%>
<hr>

<%= render 'tasks/tasks' %>