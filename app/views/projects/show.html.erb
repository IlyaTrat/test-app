<h2><%= @project.name %></h2>
<p><%= @project.summary %></p>
<p><%= @project.start_date %> - <%= @project.end_date %></p>
<%= link_to "Edit", edit_project_path(@project), :class => 'btn btn-outline-secondary'%>
<%= link_to "Delete", project_path(@project), method: :delete, data: {confirm: "Delete project?"}, :class => 'btn btn-outline-dark'%>